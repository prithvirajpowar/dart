pipeline {
  agent any
  
  stages {
    stage('Checkout') {
      steps {
        checkout scm
      }
    }
    
    stage('Compile hello.dart') {
      when {
        branch 'main'
      }
      steps {
        sh 'dart compile exe bin/hello.dart'
      }
    }
    
    stage('Compile name.dart') {
      when {
        branch 'dart'
      }
      steps {
        sh 'dart compile exe bin/name.dart'
      }
    }
  }
}

