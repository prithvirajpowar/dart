pipeline {
  agent any
  
  stages {
    stage('Checkout') {
      steps {
        checkout scm
      }
    }
    
    stage('Build_1') {
      steps {
        sh 'dart hello.dart'
      }
    }
     stage('Build_2') {
      steps {
        sh 'dart name.dart'
      }
    }
  }
}
